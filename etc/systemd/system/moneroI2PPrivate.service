[Unit]
Description=Monero I2P Bridging Node
After=network.target

[Service]
ExecStart=/bin/bash moneroI2PPrivate.sh
WorkingDirectory=/home/nanode/execScripts/
Type=simple
Restart=always
RestartSec=30
User=nanode
StandardOutput=null
StandardError=null
PIDFile=/home/nanode/monero/build/release/bin/monerod.pid

[Install]
WantedBy=multi-user.target

